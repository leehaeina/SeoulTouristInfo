{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": ["jest"],
    "extends": ["eslint:recommended", "prettier","plugin:jest/recommended"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "warn"
    }
}
